{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": [
      "import"
  ],
  "rules" : {
      "indent": ["error", 4],
      "no-underscore-dangle": ["error", { "allowAfterThis": true }],
      "comma-dangle": [
        "error",
        {
            "objects": "ignore",
            "arrays": "always-multiline"
        }
    ]
  }
}